    INSERT INTO `</span><span class="nv">$table2</span><span class="s2">` (id_react, love, likes, clapping, cool) VALUES(</span><span class="nv">$lastId</span><span class="s2">, '', '', '', '');
  "</span><span class="p">;</span>
